{
  "schemaVersion": 1,
  "id": "modid",
  "version": "${version}",
  "name": "Actionale",
  "description": "Actionale is a mod that replaces vanilla keybinds with a easily composable action system.",
  "authors": [
    "hnOsmium0001"
  ],
  "contact": {
    "homepage": "https://curseforge.com/minecraft/mc-mods/actionale",
    "sources": "https://github.com/hnOsmium0001/Actionale"
  },
  "license": "MIT",
  "icon": "assets/modid/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "adapter": "kotlin",
        "value": "io.github.hnosmium0001.actionale.Actionale"
      }
    ]
  },
  "mixins": [
    "common.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.7.4",
    "fabric": "*",
    "fabric-language-kotlin": ">=1.3.50",
    "minecraft": "1.15.x"
  },
  "suggests": {
    "flamingo": "*"
  }
}
